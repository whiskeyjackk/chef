auto lo
  iface lo inet loopback

auto eth0.10
iface eth0.10 inet static
  address 10.30.100.103
  netmask 255.255.0.0
  gateway 10.30.0.1
  dns-nameservers 10.30.0.7
  dns-search nyc3.internal.digitalocean.com

iface eth0.735 inet manual
  vlan-raw-device eth0

auto br-vlan735
iface br-vlan735 inet manual
  bridge_ports eth0.735
  bridge_stp off
  bridge_fd 0
  bridge_maxwait 0

